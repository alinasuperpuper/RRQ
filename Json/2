import json

# Создаем словарь с вашими данными
data = {
    "Фамилия": "Иванов",
    "Имя": "Иван",
    "Отчество": "Иванович",
    "Телефон": "+71234567890",
    "Год рождения": 1990,
    "Город рождения": "Москва",
    "Место учебы": "Московский государственный университет"
}

# Записываем данные в JSON-файл
with open("my_data.json", "w") as json_file:
    json.dump(data, json_file, indent=4)

print("JSON-файл успешно создан.")


#Заменить на свои данные.
